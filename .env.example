# Переменные окружения для Kuzia Bot

# Max Bot Token (получить у @masterbot)
TOKEN=your_max_bot_token_here

# AI Token для Google Gemini (получить на https://aistudio.google.com)
AI_TOKEN=your_gemini_api_key_here

# База данных
DB_URL=sqlite:///app/data/db.sqlite3

# Логирование (WARNING, ERROR, INFO, DEBUG)
LOG_LEVEL=WARNING

# Режим разработки (true/false)
DEBUG_MODE=false